<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<% if flash[:notice] %>
    <%= content_tag :h1, flash[:notice] %>
<% else %>
    <%= content_tag :h1, "Please Log In" %>
<% end %>

<%= form_with url: sessions_path do |f| %>
    <%= f.text_field :email %> <br>
    <%= f.password_field :password %> <br>
    <%= link_to "Sign up", new_user_path %>
    <%= f.submit "Sign In!" %>
<% end %>